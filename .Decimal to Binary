#include <stdio.h>
int main() {
      int n;
      scanf("%d",&n);
      
       printf("Binary equivalent of %d: ",n);
      int flag=0;
       for (int i=31;i>=0;i--){
       int res=(n>>i)&1; 
       if(res==1)
         flag=1;
        if(flag==1)
           printf("%d",res);
        }
       if(n==0){
          printf("0");
      }

    
   
}
