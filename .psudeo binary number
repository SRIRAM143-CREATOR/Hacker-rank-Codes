#include <stdio.h>
#include <string.h>

int main() {
    int n;
    scanf("%d",&n);
    int flag=0;
    while(n!=0){
        int r=n%10;
        if(r!=0 && r!=1){
            printf("No");
            flag=1;
            break;
        }
        n=n/10;
    }
    if(flag==0){
        printf("Yes");
    }
    
}
